<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>oil-press.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Katana/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <condition>
      <property>/controls/lighting/instrument-lights</property>
    </condition>
    <emission>
      <red>   0.80 </red>
      <green> 0.50 </green>
      <blue>  0.50 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine/oil-pressure-psi</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>  0.0 </dep></entry>
      <entry><ind>  30 </ind><dep> 22.5 </dep></entry>
      <entry><ind>  60 </ind><dep> 45.5 </dep></entry>
      <entry><ind>  90 </ind><dep> 69.0 </dep></entry>
      <entry><ind> 150 </ind><dep> 93.0 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.001 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.010 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
