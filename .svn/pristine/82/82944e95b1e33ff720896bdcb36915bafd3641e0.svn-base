    <path>Aircraft/Katana/Models/shadow.xml</path>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m>  0.00 </y-m>
      <z-m> -0.92 </z-m>
    </offsets>
  </model>
   
  <!-- Immatriculatoin -->
  <model>
    <path>Aircraft/Katana/Models/Immat/immat.xml</path>
  </model>

  <!-- Intérieur -->
  <model>
    <path>Aircraft/Katana/Models/Interior/interior.xml</path>
  </model>

  <!-- Cranking effects -->
  <model>
    <path>Aircraft/Katana/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -2.479 </x-m>
      <y-m>  0.101 </y-m>
      <z-m> -0.525 </z-m>
      <pitch-deg>  45 </pitch-deg>
    </offsets>
  </model>

  <!-- Effets sur le sol -->
  <!-- Roue avant -->
  <model>
    <path>Aircraft/Katana/Models/Effects/ground/ground0.xml</path>   
    <offsets>
      <x-m> -2.919 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.109 </z-m>
    </offsets>
  </model>

  <!-- Roue gauche -->
  <model>
    <path>Aircraft/Katana/Models/Effects/ground/ground1.xml</path>
    <offsets>
      <x-m> -1.222 </x-m>
      <y-m> -0.962 </y-m>
      <z-m> -1.109 </z-m>
    </offsets>
  </model>

  <!-- Roue droite -->
  <model>
    <path>Aircraft/Katana/Models/Effects/ground/ground2.xml</path>
    <offsets>
      <x-m> -1.222 </x-m>
      <y-m>  0.962 </y-m>
      <z-m> -1.109 </z-m>
    </offsets>
  </model>

  <!-- L'hélice --> 
  <animation>
    <name>HeliceComplete</name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <object-name>helice</object-name>
    <object-name>bol</object-name>
  </animation>

  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
    <and>
      <greater-than>
        <property>/engines/engine[0]/rpm</property>
        <value>600.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceComplete</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value> 5 </value>
      </greater-than>
    </condition>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -3.447 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.046 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeur</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -12.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>   9.5 </dep></entry>
    </interpolation>
    <center>
      <x-m> 3.378 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 1.085 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>direction</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -25.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  25.0 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> 2.946 </x1-m>
      <y1-m> 0.000 </y1-m>
      <z1-m> 0.113 </z1-m>
      <x2-m> 3.228 </x2-m>
      <y2-m> 0.000 </y2-m>
      <z2-m> 1.005 </z2-m>
    </axis>
  </animation>

  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep>  15.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep> -10.0 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -1.201 </x1-m>
      <y1-m> -3.257 </y1-m>
      <z1-m> -0.033 </z1-m>
      <x2-m> -1.295 </x2-m>
      <y2-m> -4.915 </y2-m>
      <z2-m>  0.083 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>aileronD</object-name>
    <property>surface-positions/right-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75 </ind><dep> -10.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>  15.0 </dep></entry>
    </interpolation>
    <axis>
      <x1-m> -1.201 </x1-m>
      <y1-m>  3.257 </y1-m>
      <z1-m> -0.033 </z1-m>
      <x2-m> -1.295 </x2-m>
      <y2-m>  4.915 </y2-m>
      <z2-m>  0.083 </z2-m>
    </axis>
  </animation>

  <!-- Volets -->
  <animation>
    <type>rotate</type>
    <object-name>voletG</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor> 20 </factor>
    <axis>
      <x1-m> -1.201 </x1-m>
      <y1-m> -3.257 </y1-m>
      <z1-m> -0.033 </z1-m>
      <x2-m> -1.065 </x2-m>
      <y2-m> -0.868 </y2-m>
      <z2-m> -0.200 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>voletD</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor> 20 </factor>
    <axis>
      <x1-m> -1.065 </x1-m>
      <y1-m>  0.868 </y1-m>
      <z1-m> -0.200 </z1-m>
      <x2-m> -1.201 </x2-m>
      <y2-m>  3.257 </y2-m>
