      <entry><ind> 1.00 </ind><dep>  4 </dep></entry>
    </interpolation>
    <center>
      <x-m> -1.278 </x-m>
      <y-m>  0.321 </y-m>
      <z-m> -0.338 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Rotation de la roue avant en fonction de la gouverne de direction -->
  <animation>
    <name>TrainAvant</name>
    <object-name>roueA</object-name>
    <object-name>cacheA</object-name>
    <object-name>intcacheA</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>TrainAvant</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 25 </factor>
    <center>
      <x-m> -3.115 </x-m>
      <y-m> -0.084 </y-m>
      <z-m> -0.785 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z> -1 </z>
    </axis>
  </animation>

  <!-- Rotation des roues lors du roulage -->
  <animation>
    <type>spin</type>
    <object-name>roueA</object-name>
    <property>/gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.919 </x-m>
      <y-m> -0.084 </y-m>
      <z-m> -0.946 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueG</object-name>
    <property>/gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -1.222 </x-m>
      <y-m> -0.962 </y-m>
      <z-m> -0.946 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueD</object-name>
    <property>/gear/gear[2]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -1.222 </x-m>
      <y-m>  0.962 </y-m>
      <z-m> -0.946 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Canopy -->
  <animation>
    <name>LaVerriere</name>
    <object-name>tourvitres</object-name>
    <object-name>vitres</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>LaVerriere</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>LaVerriere</object-name>
    <property>sim/model/door-positions/crew/position-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep> 0.0 </dep></entry>
      <entry><ind> 0.2 </ind><dep> 0.3 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 0.3 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>LaVerriere</object-name>
    <property>sim/model/door-positions/crew/position-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep> 0.0 </dep></entry>
