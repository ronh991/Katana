    </interpolation>
    <center>
      <x-m> -1.731 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.354 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

