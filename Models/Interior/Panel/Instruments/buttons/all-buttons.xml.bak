<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>all-buttons.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>fond</object-name>
    
    <object-name>text</object-name>
    <object-name>on-off0</object-name>
    <object-name>landing</object-name>
    <object-name>taxi</object-name>
    <object-name>position</object-name>
    <object-name>strobe</object-name>
    <object-name>map</object-name>
    <object-name>avionics</object-name>
    <object-name>instruments</object-name>
    <object-name>on-off1</object-name>
    <object-name>fuel-pump</object-name>
    <object-name>generator</object-name>
    <object-name>battery</object-name>
    <object-name>luminosite</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>text</object-name>
    <object-name>on-off0</object-name>
    <object-name>landing</object-name>
    <object-name>taxi</object-name>
    <object-name>position</object-name>
    <object-name>strobe</object-name>
    <object-name>map</object-name>
    <object-name>avionics</object-name>
    <object-name>instruments</object-name>
    <object-name>on-off1</object-name>
    <object-name>fuel-pump</object-name>
    <object-name>generator</object-name>
    <object-name>battery</object-name>
    <object-name>luminosite</object-name>
    <condition>
      <property>/controls/lighting/instrument-lights</property>
    </condition>
    <emission>
      <red>   0.80 </red>
      <green> 0.50 </green>
      <blue>  0.50 </blue>
    </emission>
  </animation>

  <!-- Fuel pump -->
  <animation>
    <type>pick</type>
    <object-name>fuel-pump</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine/fuel-pump</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>/sim/sound/switch</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>fuel-pump</object-name>
    <property>/controls/engines/engine/fuel-pump</property>
    <factor>  35 </factor>
    <min-deg>  0 </min-deg>
    <max-deg> 35 </max-deg>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Lights -->
  <!-- Strobe -->
  <animation>
    <type>pick</type>
    <object-name>strobe</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/lighting/strobe</property>
       </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>strobe</object-name>
    <property>/controls/lighting/strobe</property>
    <factor>  35 </factor>
    <min-deg>  0 </min-deg>
    <max-deg> 35 </max-deg>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

