    </axis>
  </animation>
   
  <!--  Throttle pick animation -->
  <animation>
    <type>pick</type>
    <object-name>luminosite</object-name>
    <visible>true</visible>
    <action>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/controls/lighting/panel-dim</property>
        <step> 0.01 </step>
        <min> 0 </min>
        <max> 1 </max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/controls/lighting/panel-dim</property>
        <step> -0.01 </step>
        <min> 0 </min>
        <max> 1 </max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>
  
  <animation> 
    <type>rotate</type>
    <object-name>luminosite</object-name>
    <property>controls/lighting/panel-dim</property>
    <factor> 270 </factor>
    <center>
      <x-m>  0.003373 </x-m>
      <y-m> -0.004490 </y-m>
      <z-m> -0.007263 </z-m>  
    </center>  
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>                                                              
 
</PropertyList>
